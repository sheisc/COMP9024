printStrs(strs){
  long i;
  long s;
  i = 0;
  s = SccRead64(strs, i*8);
  while(s){
    puts(s);
    i = i + 1;
    s = SccRead64(strs, i*8);
  }
}

main(argc, argv, env){
  output(argc);
  // Display command-line arguments
  printStrs(argv);
  // '\n'
  putchar(10);
  // Display environment variables
  printStrs(env);
  return 0;
}

